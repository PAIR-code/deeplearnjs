{
  "name": "benchmarks",
  "version": "0.1.2",
  "description": "Benchmark tools for TensorFlow.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/tensorflow/tfjs-core"
  },
  "devDependencies": {
    "@tensorflow/tfjs-core": "0.12.4",
    "@types/jasmine": "~2.5.53",
    "browserstack-runner": "~0.7.0",
    "clang-format": "~1.2.2",
    "jasmine": "~3.1.0",
    "jasmine-core": "~3.1.0",
    "firebase": "~5.3.0",
    "karma": "~2.0.2",
    "karma-browserstack-launcher": "~1.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-typescript": "~3.0.12",
    "polymer-bundler": "~3.0.1",
    "ts-node": "~7.0.0",
    "tsc": "^1.20150623.0",
    "tsify": "^3.0.4",
    "tslint": "^5.10.0",
    "typescript": "2.9.2",
    "http-server": "^0.11.1"
  },
  "scripts": {
    "build": "tsc && browserify *.ts -p [ tsify ] > bundle.js",
    "lint": "tslint -p . -t verbose",
    "server": "yarn build && http-server",
    "test": "karma start --firebaseKey $FIREBASE_KEY --travis $TRAVIS",
    "test-browserstack": "browserstack-runner",
    "test-node": "ts-node ./run_tests.ts",
    "test-travis": "karma start --firebaseKey $FIREBASE_KEY --travis $TRAVIS --singleRun --reporters='dots,karma-typescript,BrowserStack' --hostname='bs-local.com' --browsers=bs_chrome_mac",
    "test-travis-cron": "if [ \"$TRAVIS_EVENT_TYPE\" = cron ]; then karma start --singleRun --reporters='dots,karma-typescript,BrowserStack' --hostname='bs-local.com' --browsers=bs_chrome_mac; fi"
  },
  "license": "Apache-2.0",
  "engines": {
    "yarn": ">= 1.0.0"
  }
}
